@using FreeKingdomLit.Models

@model List<FreeKingdomLit.Models.Book>
<h1>Books!</h1>

@if(Model.Any())
{
<ui>
  @foreach(Book book in Model)
  {
    <li>@Html.ActionLink($"{book.Title}", "Details", new {id = book.BookId})</li>
  }
</ui>
}
else
{
  <p>no books</p>
}
<p>@Html.ActionLink("AddBook", "Create")</p>
<br>
<p>@Html.ActionLink("Back", "Index", "Home")</p>